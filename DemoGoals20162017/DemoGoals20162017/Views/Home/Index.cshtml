<script>
    var lstObj = [
        { Id: 1, Name: 'Tung', Hobboy: ['sing', 'sleep', 'travel'] },
        { Id: 2, Name: 'Tri', Hobboy: ['eat', 'drink', 'swim'] },
        { Id: 3, Name: 'Son', Hobboy: ['music', 'sing', 'learn'] }
    ];

    $(function ()
    {
        $.ajax({
            contentType: 'application/json',
            datatype: 'json',
            type: 'POST',
            url: '@Url.Action("DemoPostListObject", "Home")',
            cache: false,
            data:  JSON.stringify({ 'lst': lstObj }),
            traditional: true,

            success: function (response) {
                if (response.success) {
                    alert("Success ^_^");
                } else {
                    alert("Fail :( ");
                }
            },
            error: function (data) {

            }
        });

    });

</script>